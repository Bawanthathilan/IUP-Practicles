   "source": [
    "img1 = cv2.imread('numbers.jpg')\n",
    "fullimg = cv2.cvtColor(img1,cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "(result,threshold) = cv2.threshold(fullimg, 22, 255, cv2.THRESH_BINARY)\n",
    "cv2.imshow('Binary Threshold', threshold)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
